{
    "name": "graham-campbell/database",
    "description": "Database Provides Some Extra Database Stuff For Laravel 5",
    "keywords": ["laravel", "framework", "eloquent", "model", "repository", "Database", "Laravel Database", "Laravel-Database", "Graham Campbell", "GrahamCampbell"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "illuminate/contracts": "~5.0",
        "illuminate/database": "~5.0",
        "illuminate/events": "~5.0",
        "illuminate/pagination": "~5.0",
        "illuminate/support": "~5.0",
        "illuminate/validation": "~5.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\Database\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\Database\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
