{
    "name": "graham-campbell/database",
    "description": "Database Provides Some Extra Database Stuff For Laravel 4.2",
    "keywords": ["laravel", "framework", "eloquent", "model", "provider", "Database", "Laravel Database", "Laravel-Database", "Graham Campbell", "GrahamCampbell"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "illuminate/database": "~4.2",
        "illuminate/events": "~4.2",
        "illuminate/pagination": "~4.2",
        "illuminate/support": "~4.2",
        "illuminate/validation": "~4.2"
    },
    "require-dev": {
        "graham-campbell/testbench": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\Database\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\Database\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
